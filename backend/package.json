{
  "name": "cuenta-conmigo-backend",
  "version": "1.0.0",
  "description": "Backend API for Cuenta Conmigo - Inclusive Math Learning App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
